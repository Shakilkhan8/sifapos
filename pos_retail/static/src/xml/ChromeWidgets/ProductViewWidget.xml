<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

    <t t-name="ProductViewWidget" owl="1">
        <div class="ticket-button" t-on-click="onClick">
            <i class="fa fa-toggle-on" aria-hidden="true"  t-if="state.product_view === 'list'"/>
            <i class="fa fa-toggle-off" aria-hidden="true"  t-if="state.product_view === 'box'"/>
        </div>
    </t>

</templates>
